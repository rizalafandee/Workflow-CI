name: Liver_Prediction_Project

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      n_estimators: {type: int, default: 100}
      max_depth: {type: int, default: 10}
      min_samples_split: {type: int, default: 2}
    command: >
      python modelling.py {n_estimators} {max_depth} {min_samples_split}
